{
  "name": "@jaisocx/ts-tools-packager",
  "version": "1.2.01",
  "private": false,
  "type": "module",
  "main": "./build/CommonJS/index.cjs",
  "types": "./build/ESNext/index.d.ts",
  "module": "./build/ESNext/index.mjs",
  "exports": {
    "require": "./build/CommonJS/index.cjs",
    "import": "./build/ESNext/index.mjs"
  },
  "files": [
    "build/",
    "src/",
    "assets/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^22.10.1",
    "typescript": "^5.7.x"
  },
  "scripts": {
    "build:CommonJS": "export BUILD_MODE=CommonJS && tsc -p ./tsconfig.CommonJS.json",
    "build:ESNext": "export BUILD_MODE=ESNext && tsc -p ./tsconfig.ESNext.json",
    "watch": "tsc -w -p ./tsconfig.ESNext.json",
    "buildSubpackages:ESNext": "cd ./../../ && ./command/buildPackages.sh ESNext"
  }
}

{
  "npm-registry-name": "@jaisocx",
  "modules": [
    {
      "path": "modules/EventEmitter",
      "name": "event-emitter",
      "build": true,
      "build-simple-enable": true,
      "build-files": [
        "EventEmitter.js",
        "LargeDomEventEmitter.js"
      ]
    },
    {
      "path": "modules/TemplateRenderer",
      "name": "template-renderer",
      "build": true,
      "build-simple-enable": true,
      "dependencies": [
        {
          "name": "@jaisocx/event-emitter",
          "path": "modules/EventEmitter"
        }
      ],
      "build-files": [
        "TemplateRenderer.js"
      ]
    },
    {
      "path": "modules/Tree",
      "name": "tree",
      "build": true,
      "build-simple-enable": true,
      "dependencies": [
        {
          "name": "@jaisocx/event-emitter",
          "path": "modules/EventEmitter"
        },
        {
          "name": "@jaisocx/template-renderer",
          "path": "modules/TemplateRenderer"
        }
      ],
      "build-files": [
        "Tree.js",
        "TreeMetadata.js",
        "TreeConstants.js"
      ]
    },
    {
      "path": "examples/ExampleTemplateRenderer",
      "name": "example-template-renderer",
      "build": true,
      "build-simple-enable": true,
      "dependencies": [
        {
          "name": "@jaisocx/event-emitter",
          "path": "modules/EventEmitter"
        },
        {
          "name": "@jaisocx/template-renderer",
          "path": "modules/TemplateRenderer"
        }
      ],
      "build-files": [
        "ExampleTemplateRenderer.js"
      ]
    },
    {
      "path": "examples/ExampleTree",
      "name": "example-tree",
      "build": false,
      "build-simple-enable": true,
      "dependencies": [
        {
          "name": "@jaisocx/tree",
          "path": "modules/Tree"
        }
      ],
      "build-files": [
        "ExampleTree.js"
      ]
    }
  ]
}

